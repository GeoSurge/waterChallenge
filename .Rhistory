library(tidyverse)
school_lookup <- read_csv('sfusd_lead_sampling.csv')
glimpse(school_lookup)
school_lookup %>%
select(geo_school_name, lat, long) %>%
unique() %>%
mutate(id = row_number())
school_lookup %>%
select(geo_school_name, lat, lon) %>%
unique() %>%
mutate(id = row_number())
school_lookup %>%
select(geo_school_name, lat, lon) %>%
unique() %>%
mutate(id = row_number()) %>%
select(id, geo_school_name:lon) %>%
write_csv('school_lookup.csv')
school_lookup %>%
select(geo_school_name, lat, lon) %>%
unique() %>%
mutate(id = row_number()) %>%
select(id, geo_school_name:lon)
glimpse(school_lookup)
school_lookup %>%
group_by(school_name) %>%
mutate(lead = ifelse(`Pb (µg/L)` == '< 1' | is.na(`Pb (µg/L)`), FALSE)) %>% glimpse()
school_lookup %>%
group_by(school_name) %>%
mutate(lead = ifelse(`Pb (µg/L)` == '< 1' | is.na(`Pb (µg/L)`), FALSE, TRUE)) %>% glimpse()
school_lookup %>%
group_by(school_name) %>%
mutate(lead = ifelse(`Pb (µg/L)` == '< 1' | is.na(`Pb (µg/L)`), FALSE, TRUE)) %>% View()
school_lookup %>%
group_by(school_name) %>%
mutate(lead = ifelse(`Pb (µg/L)` == '< 1' | is.na(`Pb (µg/L)`), FALSE, TRUE)) %>%
select(school_name, lat, lon, `Pb (µg/L)`, lead) %>% View
school_lookup %>%
mutate(lead_present = ifelse(`Pb (µg/L)` == '< 1' | is.na(`Pb (µg/L)`), FALSE, TRUE)) %>%
group_by(school_name, lat, lon) %>%
mutate(lead_present2 = any(lead_present)) %>%
select(school_name, lat, lon, lead_present, lead_present2) %>% View
school_lookup %>%
mutate(lead_present = ifelse(`Pb (µg/L)` == '< 1' | is.na(`Pb (µg/L)`), FALSE, TRUE)) %>%
group_by(school_name, lat, lon) %>%
summarise(lead_present = any(lead_present)) %>%
mutate(id = row_number()) %>% View
school_lookup %>%
mutate(lead_present = ifelse(`Pb (µg/L)` == '< 1' | is.na(`Pb (µg/L)`), FALSE, TRUE)) %>%
group_by(school_name, lat, lon) %>%
summarise(lead_present = any(lead_present)) %>%
ungroup() %>%
mutate(id = row_number()) %>% View
school_lookup %>%
mutate(lead_present = ifelse(`Pb (µg/L)` == '< 1' | is.na(`Pb (µg/L)`), FALSE, TRUE)) %>%
group_by(school_name, lat, lon) %>%
summarise(lead_present = any(lead_present)) %>%
ungroup() %>%
mutate(id = row_number()) %>%
select(id, lat:lead_present)
school_lookup %>%
mutate(lead_present = ifelse(`Pb (µg/L)` == '< 1' | is.na(`Pb (µg/L)`), FALSE, TRUE)) %>%
group_by(school_name, lat, lon) %>%
summarise(lead_present = any(lead_present)) %>%
ungroup() %>%
mutate(id = row_number()) %>%
select(id, school_name:lead_present)
school_lookup %>%
mutate(lead_present = ifelse(`Pb (µg/L)` == '< 1' | is.na(`Pb (µg/L)`), FALSE, TRUE)) %>%
group_by(school_name, lat, lon) %>%
summarise(lead_present = any(lead_present)) %>%
ungroup() %>%
mutate(id = row_number()) %>%
select(id, school_name:lead_present) %>%
write_csv('school_lookup.csv')
